import java.util.*;
public class SlideingWindow
{
    public static void main(String[] args)
{
    Scanner sc=new Scanner(System.in)
    int n=sc.nextInt();
    int[] a=new int[n];
    for(int i=0;i<n;i++)
    {
      a[i]=sc.nextInt();
    }
    int i=0;
    int j=0;
    while(j<a.length)
{  
   for(int k=i;k<=j;k++)
{
  System.out.print(a[k]+ " ");
}
   System.out.println();
   i++; 
   j++;
}
}
